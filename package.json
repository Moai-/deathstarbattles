{
  "name": "death-star-battles",
  "version": "0.9.1",
  "description": "Re-implementation of the classic Death Star Battles Java game by Ian Bollard",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "cd frontend && yarn typecheck",
    "dev": "cd frontend && vite",
